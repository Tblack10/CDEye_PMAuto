<?xml version="1.0" encoding="UTF-8"?>

<faces-config
        xmlns="http://java.sun.com/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
        version="2.0">

    <navigation-rule>
        <from-view-id>/WPList.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>EDIT_WP_LEAF</from-outcome>
            <to-view-id>/EditWorkPackageLeaf.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/EditWorkPackageLeaf.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>RETURN_WP_LIST</from-outcome>
            <to-view-id>/WPList.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/CreateProject.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>VIEW_PROJECTS</from-outcome>
            <to-view-id>/ProjectDashboard.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>


    <application>
      <resource-bundle>
         <base-name>messages</base-name>
         <var>msgs</var>
      </resource-bundle>
   </application>
</faces-config>